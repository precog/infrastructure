server {
  port = 30050
  sslPort = 30051
  sslEnable = false

  log {
    level = "info"
    console = true
    filename = "/var/log/reportgrid/vistrack-v1.server.log"
    roll = "daily"
    use_parents = false
  }
}

services {
  vistrack {
    v1 {
      mongo {
        database = "analytics1"
        servers = ["devmongo01.reportgrid.com:27017"]
      }
    }
  }
}
